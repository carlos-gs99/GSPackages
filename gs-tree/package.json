{
  "name": "@carlos-gs99/gs-tree",
  "version": "1.0.0",
  "description": "Accessible hierarchical tree view component with expand/collapse and selection",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-i18next": "^15.0.0",
    "@carlos-gs99/hooks": "^1.0.0",
    "@carlos-gs99/utils": "^1.0.0",
    "@carlos-gs99/gs-icon": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "react": "^18.3.1",
    "react-i18next": "^15.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "@carlos-gs99/hooks": "^1.0.0",
    "@carlos-gs99/utils": "^1.0.0",
    "@carlos-gs99/gs-icon": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "sideEffects": ["*.css"]
